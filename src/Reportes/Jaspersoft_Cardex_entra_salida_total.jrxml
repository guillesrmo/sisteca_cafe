	<parameter name="id_producto" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="FECHA1" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="FECHA2" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="almacen" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select ISNULL((SELECT
     sum(det_comp."det_com_pro_cantidad" *
     present."present_cantidad")
FROM
     "producto_presentacion" present INNER JOIN "producto" produt ON present."present_cod_producto" = produt."producto_id"
     INNER JOIN "detalle_compra_producto" det_comp ON present."present_id" = det_comp."det_com_pro_id_producto"
	 inner join compra compr on compr.compra_id=det_comp.det_com_pro_id_compra
WHERE
     produt.producto_id =producto.producto_id and compr.compra_fecha_emis BETWEEN $P{FECHA1} AND $P{FECHA2} and  compr.compra_almacen=$P{almacen}),0)as entrada,isNULL(( SELECT sum(det_ven."det_ven_pro_cantidad"*
     present."present_cantidad" ) AS total
	 FROM
     "producto_presentacion" present INNER JOIN "producto" produt ON present."present_cod_producto" = produt."producto_id"
     INNER JOIN "detalle_venta_producto" det_ven ON present."present_id" = det_ven."det_ven_pro_id_producto"
	 INNER JOIN venta vent on vent.venta_id=det_ven.det_ven_pro_id_venta
	 WHERE
     produt.producto_id =producto.producto_id AND  vent.venta_fecha_emision BETWEEN $P{FECHA1} AND $P{FECHA2}  and  vent.venta_empresa=$P{almacen}),0) as salida,
	 ( ISNULL((SELECT
     sum(det_comp."det_com_pro_cantidad" *
     present."present_cantidad")
FROM
     "producto_presentacion" present INNER JOIN "producto" produt ON present."present_cod_producto" = produt."producto_id"
     INNER JOIN "detalle_compra_producto" det_comp ON present."present_id" = det_comp."det_com_pro_id_producto"
	 inner join compra compr on compr.compra_id=det_comp.det_com_pro_id_compra
WHERE
     produt.producto_id =producto.producto_id and compr.compra_fecha_emis BETWEEN $P{FECHA1} AND $P{FECHA2} and  compr.compra_almacen=$P{almacen}) ,0)-isNULL(( SELECT sum(det_ven."det_ven_pro_cantidad"*
     present."present_cantidad" ) AS total
	 FROM
     "producto_presentacion" present INNER JOIN "producto" produt ON present."present_cod_producto" = produt."producto_id"
     INNER JOIN "detalle_venta_producto" det_ven ON present."present_id" = det_ven."det_ven_pro_id_producto"
	 INNER JOIN venta vent on vent.venta_id=det_ven.det_ven_pro_id_venta
	 WHERE
     produt.producto_id =producto.producto_id AND  vent.venta_fecha_emision BETWEEN $P{FECHA1} AND $P{FECHA2}  and  vent.venta_empresa=$P{almacen}),0)  ) as total,producto_id from producto where
    producto.producto_id = $P{id_producto}]]>
	</queryString>
